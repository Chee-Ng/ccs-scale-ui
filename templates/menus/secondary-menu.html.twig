<ul class="nav-list nav-list--secondary" role="navigation">
    {% for menu_item in menu.children %}
        <li class="nav-list__item">
            <a class="nav-list__link {% if menu_item.active %} item-current {% endif %} " 
            href="{{ menu_item.url|modify_url }}">{{ menu_item.label }}</a>
        </li>
    {% endfor %}
</ul>
