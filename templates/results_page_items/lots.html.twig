 {% if  lotsData[framework.number] is defined %}  
    {% for lot in  lotsData[framework.number] %}
        <div class ='gm-lots-containers'>
        <h3 class="govuk-heading-m">{{lot.number}}: {{lot.name}}</h3>
        <p> {{lotsData[framework.number][lot.number].description|raw}} </p>
        <div>
         {% if  isScale == true %}  
            Start Procurement on {{lot.number}}
        {% endif %}  

        </div>
    </div>
    {% endfor %}
{% endif %}  
